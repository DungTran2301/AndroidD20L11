<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment2">

    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.dungtran.androidd20l11.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment2_to_musicFragment2"
            app:destination="@id/musicFragment2" />
    </fragment>
    <fragment
        android:id="@+id/musicFragment2"
        android:name="com.dungtran.androidd20l11.MusicFragment"
        android:label="fragment_music"
        tools:layout="@layout/fragment_music" >
        <action
            android:id="@+id/action_musicFragment2_to_favouriteFragment"
            app:destination="@id/favouriteFragment" />
        <action
            android:id="@+id/action_musicFragment2_to_myMusicFragment"
            app:destination="@id/myMusicFragment" />
    </fragment>
    <fragment
        android:id="@+id/favouriteFragment"
        android:name="com.dungtran.androidd20l11.FavouriteFragment"
        android:label="fragment_favourite"
        tools:layout="@layout/fragment_favourite" />
    <fragment
        android:id="@+id/myMusicFragment"
        android:name="com.dungtran.androidd20l11.MyMusicFragment"
        android:label="fragment_my_music"
        tools:layout="@layout/fragment_my_music" />
</navigation>